{
  "name": "politico",
  "version": "1.0.0",
  "description": "Politico enables citizens give their mandate to politicians running for different government offices while building trust in the process through transparency.",
  "main": "./server/app.js",
  "scripts": {
    "build": "babel server --out-dir ./dist --source-maps",
    "debug": "node --inspect-brk ./dist/index.js",
    "serve": "node ./dist/index.js",
    "start": "nodemon --exec babel-node ./server/app.js",
    "test": "./node_modules/.bin/mocha ./server/tests/* --require @babel/register --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haywhyze/politico.git"
  },
  "author": "Yusuf Ayo Abdulkarim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haywhyze/politico/issues"
  },
  "homepage": "https://github.com/haywhyze/politico#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.1"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4"
  }
}
